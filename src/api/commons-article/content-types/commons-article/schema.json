{
  "kind": "collectionType",
  "collectionName": "commons_articles",
  "info": {
    "singularName": "commons-article",
    "pluralName": "commons-articles",
    "displayName": "Articles Communs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "label": {
      "type": "string",
      "maxLength": 60,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subtitle": {
      "type": "string",
      "maxLength": 60,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cta": {
      "type": "string",
      "default": "DÃ©couvrir",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "focusImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "bannerImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "buttons.download-button",
        "buttons.external-link",
        "carousels.illustrations",
        "carousels.key-data",
        "carousels.media",
        "lists.checklist",
        "media.image",
        "media.video-player",
        "texts.accordion",
        "texts.chapter",
        "texts.illustrated-text",
        "texts.quote",
        "texts.richtext",
        "texts.title",
        "core.section-separator",
        "lists.partners",
        "media.illustrate-video",
        "texts.unillustrated-text",
        "carousels.temoignages",
        "lists.list-with-icons"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "summary": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "core.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "textBeforeFooter": {
      "type": "component",
      "repeatable": false,
      "component": "texts.text-before-footer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "websites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "headerCta": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "buttons.generic-cta"
    }
  }
}
