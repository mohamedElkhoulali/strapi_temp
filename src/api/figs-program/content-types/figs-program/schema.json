{
  "kind": "singleType",
  "collectionName": "figs_programs",
  "info": {
    "singularName": "figs-program",
    "pluralName": "figs-programs",
    "displayName": "[FIGS] Programmes",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "headline": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "core.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "course.programs-category"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "articlesOrder": {
      "type": "string",
      "required": false,
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "textBeforeFooter": {
      "type": "component",
      "repeatable": false,
      "component": "texts.text-before-footer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::figs-article.figs-article"
    }
  }
}
