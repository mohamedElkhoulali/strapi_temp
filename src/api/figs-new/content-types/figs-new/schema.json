{
  "kind": "collectionType",
  "collectionName": "figs_news",
  "info": {
    "singularName": "figs-new",
    "pluralName": "figs-news",
    "displayName": "[FIGS] Actualit√©s",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "headline": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "keywords": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "summary": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "focusImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "bannerImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "buttons.download-button",
        "buttons.external-link",
        "buttons.internal-link",
        "carousels.campus",
        "carousels.illustrations",
        "carousels.key-data",
        "carousels.media",
        "course.programs-category",
        "lists.checklist",
        "media.image",
        "media.video-player",
        "texts.accordion",
        "texts.chapter",
        "texts.illustrated-text",
        "texts.quote",
        "texts.richtext",
        "texts.title",
        "core.section-separator",
        "teams.figs-team",
        "lists.partners",
        "media.illustrate-video",
        "news.news-list",
        "carousels.carousel-d-evenements",
        "carousels.carousel-actus-et-agenda",
        "carousels.carousel-de-lives",
        "carousels.temoignages"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "core.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "publishDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "textBeforeFooter": {
      "type": "component",
      "repeatable": false,
      "component": "texts.text-before-footer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cat_news": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::figs-cat-new.figs-cat-new",
      "mappedBy": "news"
    },
    "campus_infos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::figs-campus-info.figs-campus-info",
      "inversedBy": "actualites"
    }
  }
}
