        "media.image",
        "media.video-player",
        "texts.accordion",
        "texts.chapter",
        "texts.illustrated-text",
        "texts.quote",
        "texts.richtext",
        "texts.title",
        "core.section-separator",
        "media.illustrate-video",
        "carousels.temoignages"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
